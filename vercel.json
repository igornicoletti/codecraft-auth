{
  "version": 2,
  "buildCommand": "pnpm build",
  "outputDirectory": "dist",
  "builds": [
    {
      "src": "src/entry.server.tsx",
      "use": "@vercel/node"
    },
    {
      "src": "dist/(.*)",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/entry.server.tsx"
    }
  ]
}
