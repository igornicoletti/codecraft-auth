{
  "version": 2,
  "buildCommand": "pnpm build",
  "outputDirectory": "dist",
  "builds": [
    {
      "src": "dist/entry.server.js",
      "use": "@vercel/static"
    },
    {
      "src": "dist/(.*)",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/chunks/(.*)",
      "dest": "/dist/chunks/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/dist/assets/$1"
    },
    {
      "src": "/codecraft.svg",
      "dest": "/dist/codecraft.svg"
    },
    {
      "src": "/favicon.ico",
      "dest": "/dist/codecraft.svg"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/entry.server.js"
    }
  ]
}
